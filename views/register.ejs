<% include partials/header.ejs %>
<div class="container ml-12 pt-5">
  <h3 class="pt-5">Register Page</h3>
</div>
<form action="/register" method="POST" class="container">
  <input
    type="text"
    name="username"
    class="form-control"
    placeholder="Username"
  /><br />
  <input
    type="text"
    name="email"
    class="form-control"
    placeholder="Email"
  /><br />
  <input
    type="password"
    name="password"
    class="form-control"
    placeholder="Password"
  /><br />
  <button type="submit" class="btn btn-primary btn-block">Submit</button>
</form>
<% include partials/footer.ejs %>
